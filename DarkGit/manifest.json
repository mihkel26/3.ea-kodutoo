{
  "name": "Dark Github",
  "description": "Darkmode for Github",
  "version": "1.0",
  "permissions": ["tabs", "*://github.com/*"],
  "content_scripts":[ {
    "run_at" : "document_idle",
    "matches": ["*://github.com/*"],
    "css": ["styles.css"]
  } ],
  "browser_action": {
    "default_title": "Go Darkmode",
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}

